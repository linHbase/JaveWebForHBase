<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" 
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" 
				id="WebApp_ID" version="3.0">
  <display-name>HBase system</display-name>
  <welcome-file-list>
    <welcome-file>manager.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
    <servlet-name>HBaseLogin</servlet-name>
    <servlet-class>cug.HBaseLogin</servlet-class>
  </servlet>
   <servlet>
    <servlet-name>HBaseLoginOut</servlet-name>
    <servlet-class>cug.HBaseLoginOut</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>HBaseImport</servlet-name>
    <servlet-class>cug.MapReduceInput</servlet-class>
  </servlet>
   <servlet>
    <servlet-name>searchByTime</servlet-name>
    <servlet-class>cug.searchByTime</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>searchByMantics</servlet-name>
    <servlet-class>cug.searchByMantics</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>searchByTimeAndRange</servlet-name>
    <servlet-class>cug.searchByTimeAndRange</servlet-class>
  </servlet>
  
   <servlet-mapping>
    <servlet-name>HBaseLogin</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>  
   <servlet-mapping>
    <servlet-name>HBaseLoginOut</servlet-name>
    <url-pattern>/loginOut</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>HBaseImport</servlet-name>
    <url-pattern>/login/Import</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
    <servlet-name>searchByTime</servlet-name>
    <url-pattern>/login/searchByTime</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
   <servlet-name>searchByMantics</servlet-name>
    <url-pattern>/login/searchByMantics</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
   <servlet-name>searchByTimeAndRange</servlet-name>
    <url-pattern>/login/searchByTimeAndRange</url-pattern>
  </servlet-mapping>

</web-app>